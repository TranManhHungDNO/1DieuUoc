INSERT INTO `ctypa_vi_news_1` (`id`, `catid`, `listcatid`, `topicid`, `admin_id`, `author`, `sourceid`, `addtime`, `edittime`, `status`, `publtime`, `exptime`, `archive`, `title`, `alias`, `hometext`, `homeimgfile`, `homeimgalt`, `homeimgthumb`, `inhome`, `allowed_comm`, `allowed_rating`, `hitstotal`, `hitscm`, `total_rating`, `click_rating`) VALUES
(1, 1, '1', 0, 1, '', 1, 1430433422, 1430433251, 1, 1430433251, 0, 1, 'Hành hương từ nửa đêm về đất Tổ', 'Hanh-huong-tu-nua-dem-ve-dat-To', '3h sáng mùng 10&#x002F;3, gia đình chị Vân &#40;Hà Nội&#41; gồm 11 thành viên, người già nhất 60 tuổi, trẻ là 5, rồng rắn nhau về Đền Hùng. Nhiều gia đình khác ngủ qua đêm dưới chân núi để kịp hành hương vào sáng sớm.', '2015_05/01/8707498525403-1.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(2, 1, '1', 0, 1, '', 1, 1430433422, 1430433004, 1, 1430433004, 0, 1, '13 chuyến bay không thể hạ cánh tại Tân Sơn Nhất vì mưa giông', '13-chuyen-bay-khong-the-ha-canh-tai-Tan-Son-Nhat-vi-mua-giong', 'Mưa lớn đột ngột chiều 28&#x002F;4 ở TP HCM khiến hàng loạt chuyến bay phải hạ cánh xuống các sân bay dự bị như Cần Thơ, Cam Ranh, Đà Nẵng, PhnomPenh thay vì ở Tân Sơn Nhất.', '2015_05/01/4365449373089-4.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(3, 1, '1', 0, 1, '', 1, 1430433422, 1430433010, 1, 1430433010, 0, 1, 'Chi 5.000 tỷ đồng nâng cấp đường, Sài Gòn đi Đà Lạt chỉ còn 5 giờ', 'Chi-5-000-ty-dong-nang-cap-duong-Sai-Gon-di-Da-Lat-chi-con-5-gio', 'Cùng với đường cao tốc TP HCM - Long Thành - Dầu Giây, dự án quốc lộ 20 đi vào hoạt động sẽ giảm thời gian đi từ TP HCM đến Đà Lạt 3 tiếng so với trước đây.', '2015_05/01/67681641267266-5.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(4, 1, '1', 0, 1, '', 1, 1430433422, 1430432994, 1, 1430432994, 0, 1, 'Hơn 30 trẻ mầm non nhập viện nghi ăn bánh mì nhiễm khuẩn', 'Hon-30-tre-mam-non-nhap-vien-nghi-an-banh-mi-nhiem-khuan', 'Sau bữa chiều với bánh mì ngọt, 34 học sinh trường mầm non ở Cần Thơ liên tục ói, tiêu chảy và sốt.', '2015_05/01/95320378091-6.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(5, 1, '1', 0, 1, '', 1, 1430433422, 1430433386, 1, 1430433386, 0, 1, 'Nhóm người Việt đầu tiên rời Nepal an toàn', 'Nhom-nguoi-Viet-dau-tien-roi-Nepal-an-toan', 'Sang Nepal học hỏi kinh nghiệm ứng phó với động đất, đoàn 10 người của Hội chữ thập đỏ Việt Nam đã gặp thảm họa thật xảy ra.', '2015_05/01/32325888278358-7.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(6, 1, '1', 0, 1, '', 1, 1430433422, 1430433055, 1, 1430433055, 0, 1, 'Nỗi niềm gia đình có người ở hai bên chiến tuyến', 'Noi-niem-gia-dinh-co-nguoi-o-hai-ben-chien-tuyen', 'Chiến tranh, ba người cháu của Bí thư thành ủy đầu tiên của Đà Nẵng đứng ở hai đầu chiến tuyến. Một trong số hai người làm việc cho chế độ Việt Nam Cộng hòa là tử sĩ trong trận hải chiến Hoàng Sa.', '2015_05/01/1612494558450-8.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(7, 1, '1', 0, 1, '', 1, 1430433422, 1430433245, 1, 1430433245, 0, 1, 'Hai người thiệt mạng do bom bi phát nổ', 'Hai-nguoi-thiet-mang-do-bom-bi-phat-no', 'Một quả bom bi phát nổ khiến hai người đàn ông đang khai thác bạch đàn trong cánh rừng thuộc xã Quảng Hợp &#40;huyện Quảng Trạch, Quảng Bình&#41; thiệt mạng, hai người khác bị thương.', '2015_05/01/7245996774107-9.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(8, 1, '1', 0, 1, '', 1, 1430433422, 1430433134, 1, 1430433134, 0, 1, 'Cảnh sát giao thông bị xe đầu kéo cuốn vào gầm', 'Canh-sat-giao-thong-bi-xe-dau-keo-cuon-vao-gam', 'Rạng sáng 29&#x002F;4, thượng uý CSGT Đội Phú Lâm &#40;TP HCM&#41; bị xe đầu kéo tông, cuốn vào gầm khi đang làm nhiệm vụ tuần tra, kiểm soát trên quốc lộ 1A.', '2015_05/01/4075197970-12.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(9, 1, '1', 0, 1, '', 1, 1430433422, 1430433555, 1, 1430433555, 0, 1, 'Quán phở Việt giúp hàng chục người gặp nạn ở Nepal', 'Quan-pho-Viet-giup-hang-chuc-nguoi-gap-nan-o-Nepal', 'Mấy ngày qua cửa hàng Phở 99 ở Jhamsikhel &#40;Kathmandu&#41; của chị Cương đã đón hàng chục người Việt tại Nepal đến tá túc, kể từ sau thảm họa động đất.', '2015_05/01/351432069305906-14.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(10, 1, '1', 0, 1, '', 1, 1430433422, 1430433347, 1, 1430433347, 0, 1, 'Lãnh đạo Đảng, Nhà nước tưởng niệm các Anh hùng liệt sĩ tại TP HCM', 'Lanh-dao-Dang-Nha-nuoc-tuong-niem-cac-Anh-hung-liet-si-tai-TP-HCM', 'Chiều 29&#x002F;4, Tổng bí thư Nguyễn Phú Trọng dẫn đầu đoàn lãnh đạo Đảng, Nhà nước đến dâng hương tưởng niệm tại Nghĩa trang Liệt sĩ TP HCM.', '2015_05/01/74902015015153-15.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(11, 1, '1', 0, 1, '', 1, 1430433422, 1430433324, 1, 1430433324, 0, 1, 'Tìm thấy trung tá phi công trong buồng lái tiêm kích Su-22', 'Tim-thay-trung-ta-phi-cong-trong-buong-lai-tiem-kich-Su-22', 'Sau 12 ngày gặp nạn cùng đồng đội tại vùng biển Phú Quý &#40;Bình Thuận&#41;, trung tá Lê Văn Nghĩa được tìm thấy cách vị trí rơi chừng 100 mét. Bộ Quốc phòng tiếp tục tìm kiếm phi công Nguyễn Anh Tú. ', '2015_05/01/0309638823032-16.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(12, 1, '1', 0, 1, '', 1, 1430433422, 1430433559, 1, 1430433559, 0, 1, 'Lắp pháo hoa trên toà nhà cao nhất TP HCM', 'Lap-phao-hoa-tren-toa-nha-cao-nhat-TP-HCM', 'Hàng chục chuyên gia kỹ thuật cùng lực lượng Z121 đang tất bật lắp pháo hoa, chuẩn bị cho màn trình diễn chào mừng 40 năm thống nhất đất nước lúc 21h45 ngày 30&#x002F;4.', '2015_05/01/57825157962994-17.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(13, 1, '1', 0, 1, '', 1, 1430433422, 1430433167, 1, 1430433167, 0, 1, 'Hàng nghìn người Sài Gòn đến quảng trường đi bộ Nguyễn Huệ', 'Hang-nghin-nguoi-Sai-Gon-den-quang-truong-di-bo-Nguyen-Hue', 'Sáng 29&#x002F;4, phố đi bộ đầu tiên của Việt Nam mở cửa cho hàng nghìn người đến chụp ảnh, tham quan.', '2015_05/01/332531040342614-19.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(14, 1, '1', 0, 1, '', 1, 1430433422, 1430433495, 1, 1430433495, 0, 1, 'Năm người tử nạn khi xe Camry bị ôtô khách đâm bẹp dúm', 'Nam-nguoi-tu-nan-khi-xe-Camry-bi-oto-khach-dam-bep-dum', 'Sau cú tông ngang, xe khách đẩy chiếc Camry chở 7 người đi chừng 100 mét mới dừng lại. 5 nạn nhân tử vong, 2 người bị thương. Chiếc Camry bẹp dúm, gần như gãy đôi.', '2015_05/01/995912092909556-20.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(15, 1, '1', 0, 1, '', 1, 1430433422, 1430433442, 1, 1430433442, 0, 1, 'Người phụ nữ bị nước cuốn mất tích tại trung tâm Đà Lạt', 'Nguoi-phu-nu-bi-nuoc-cuon-mat-tich-tai-trung-tam-Da-Lat', 'Sau cơn mưa lớn, chị Duyên ra sau nhà dọn dẹp thì trượt chân xuống dòng suối đang chảy cuồn cuộn, mất tích.', '2015_05/01/01453455780247-21.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(16, 1, '1', 0, 1, '', 1, 1430433422, 1430433675, 1, 1430433675, 0, 1, 'Tìm thấy thi thể phi công thứ hai của tiêm kích Su-22', 'Tim-thay-thi-the-phi-cong-thu-hai-cua-tiem-kich-Su-22', 'Sau 13 ngày gặp nạn cùng đồng đội ở vùng biển Phú Qúy &#40;Bình Thuận&#41;, thi thể đại úy Nguyễn Anh Tú được tìm thấy cách vị trí tiêm kích Su-22 rơi hơn 200 mét.', '2015_05/01/895085435801487-23.jpg', '', 2, 1, '4', 1, 1, 0, 0, 0),
(17, 1, '1', 0, 1, '', 1, 1430433422, 1430433455, 1, 1430433455, 0, 1, '5 ôtô húc nhau tại cửa ngõ Sài Gòn', '5-oto-huc-nhau-tai-cua-ngo-Sai-Gon', 'Sáng 30&#x002F;4, xe khách được cho là lao tới đâm ôtô phía trước, gây tai nạn liên hoàn cho 3 xe nữa.', '2015_05/01/7598374999081-27.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(18, 1, '1', 0, 1, '', 1, 1430433422, 1430433447, 1, 1430433447, 0, 1, 'Những chính sách có hiệu lực từ tháng 5', 'Nhung-chinh-sach-co-hieu-luc-tu-thang-5', 'Hỗ trợ duy trì việc làm cho người lao động, kết luận thanh tra phải được kiểm tra, giám sát, văn phòng công chứng ở địa bàn khó khăn được ưu đãi... là những quy định có hiệu lực từ 1&#x002F;5.', '2015_05/01/73086314469855-28.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0);

