INSERT INTO `ctypa_vi_news_rows` (`id`, `catid`, `listcatid`, `topicid`, `admin_id`, `author`, `sourceid`, `addtime`, `edittime`, `status`, `publtime`, `exptime`, `archive`, `title`, `alias`, `hometext`, `homeimgfile`, `homeimgalt`, `homeimgthumb`, `inhome`, `allowed_comm`, `allowed_rating`, `hitstotal`, `hitscm`, `total_rating`, `click_rating`) VALUES
(1, 1, '1', 0, 1, '', 1, 1430433422, 1430433251, 1, 1430433251, 0, 1, 'Hành hương từ nửa đêm về đất Tổ', 'Hanh-huong-tu-nua-dem-ve-dat-To', '3h sáng mùng 10&#x002F;3, gia đình chị Vân &#40;Hà Nội&#41; gồm 11 thành viên, người già nhất 60 tuổi, trẻ là 5, rồng rắn nhau về Đền Hùng. Nhiều gia đình khác ngủ qua đêm dưới chân núi để kịp hành hương vào sáng sớm.', '2015_05/01/8707498525403-1.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(2, 1, '1', 0, 1, '', 1, 1430433422, 1430433004, 1, 1430433004, 0, 1, '13 chuyến bay không thể hạ cánh tại Tân Sơn Nhất vì mưa giông', '13-chuyen-bay-khong-the-ha-canh-tai-Tan-Son-Nhat-vi-mua-giong', 'Mưa lớn đột ngột chiều 28&#x002F;4 ở TP HCM khiến hàng loạt chuyến bay phải hạ cánh xuống các sân bay dự bị như Cần Thơ, Cam Ranh, Đà Nẵng, PhnomPenh thay vì ở Tân Sơn Nhất.', '2015_05/01/4365449373089-4.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(3, 1, '1', 0, 1, '', 1, 1430433422, 1430433010, 1, 1430433010, 0, 1, 'Chi 5.000 tỷ đồng nâng cấp đường, Sài Gòn đi Đà Lạt chỉ còn 5 giờ', 'Chi-5-000-ty-dong-nang-cap-duong-Sai-Gon-di-Da-Lat-chi-con-5-gio', 'Cùng với đường cao tốc TP HCM - Long Thành - Dầu Giây, dự án quốc lộ 20 đi vào hoạt động sẽ giảm thời gian đi từ TP HCM đến Đà Lạt 3 tiếng so với trước đây.', '2015_05/01/67681641267266-5.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(4, 1, '1', 0, 1, '', 1, 1430433422, 1430432994, 1, 1430432994, 0, 1, 'Hơn 30 trẻ mầm non nhập viện nghi ăn bánh mì nhiễm khuẩn', 'Hon-30-tre-mam-non-nhap-vien-nghi-an-banh-mi-nhiem-khuan', 'Sau bữa chiều với bánh mì ngọt, 34 học sinh trường mầm non ở Cần Thơ liên tục ói, tiêu chảy và sốt.', '2015_05/01/95320378091-6.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(5, 1, '1', 0, 1, '', 1, 1430433422, 1430433386, 1, 1430433386, 0, 1, 'Nhóm người Việt đầu tiên rời Nepal an toàn', 'Nhom-nguoi-Viet-dau-tien-roi-Nepal-an-toan', 'Sang Nepal học hỏi kinh nghiệm ứng phó với động đất, đoàn 10 người của Hội chữ thập đỏ Việt Nam đã gặp thảm họa thật xảy ra.', '2015_05/01/32325888278358-7.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(6, 1, '1', 0, 1, '', 1, 1430433422, 1430433055, 1, 1430433055, 0, 1, 'Nỗi niềm gia đình có người ở hai bên chiến tuyến', 'Noi-niem-gia-dinh-co-nguoi-o-hai-ben-chien-tuyen', 'Chiến tranh, ba người cháu của Bí thư thành ủy đầu tiên của Đà Nẵng đứng ở hai đầu chiến tuyến. Một trong số hai người làm việc cho chế độ Việt Nam Cộng hòa là tử sĩ trong trận hải chiến Hoàng Sa.', '2015_05/01/1612494558450-8.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(7, 1, '1', 0, 1, '', 1, 1430433422, 1430433245, 1, 1430433245, 0, 1, 'Hai người thiệt mạng do bom bi phát nổ', 'Hai-nguoi-thiet-mang-do-bom-bi-phat-no', 'Một quả bom bi phát nổ khiến hai người đàn ông đang khai thác bạch đàn trong cánh rừng thuộc xã Quảng Hợp &#40;huyện Quảng Trạch, Quảng Bình&#41; thiệt mạng, hai người khác bị thương.', '2015_05/01/7245996774107-9.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(8, 1, '1', 0, 1, '', 1, 1430433422, 1430433134, 1, 1430433134, 0, 1, 'Cảnh sát giao thông bị xe đầu kéo cuốn vào gầm', 'Canh-sat-giao-thong-bi-xe-dau-keo-cuon-vao-gam', 'Rạng sáng 29&#x002F;4, thượng uý CSGT Đội Phú Lâm &#40;TP HCM&#41; bị xe đầu kéo tông, cuốn vào gầm khi đang làm nhiệm vụ tuần tra, kiểm soát trên quốc lộ 1A.', '2015_05/01/4075197970-12.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(9, 1, '1', 0, 1, '', 1, 1430433422, 1430433555, 1, 1430433555, 0, 1, 'Quán phở Việt giúp hàng chục người gặp nạn ở Nepal', 'Quan-pho-Viet-giup-hang-chuc-nguoi-gap-nan-o-Nepal', 'Mấy ngày qua cửa hàng Phở 99 ở Jhamsikhel &#40;Kathmandu&#41; của chị Cương đã đón hàng chục người Việt tại Nepal đến tá túc, kể từ sau thảm họa động đất.', '2015_05/01/351432069305906-14.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(10, 1, '1', 0, 1, '', 1, 1430433422, 1430433347, 1, 1430433347, 0, 1, 'Lãnh đạo Đảng, Nhà nước tưởng niệm các Anh hùng liệt sĩ tại TP HCM', 'Lanh-dao-Dang-Nha-nuoc-tuong-niem-cac-Anh-hung-liet-si-tai-TP-HCM', 'Chiều 29&#x002F;4, Tổng bí thư Nguyễn Phú Trọng dẫn đầu đoàn lãnh đạo Đảng, Nhà nước đến dâng hương tưởng niệm tại Nghĩa trang Liệt sĩ TP HCM.', '2015_05/01/74902015015153-15.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(11, 1, '1', 0, 1, '', 1, 1430433422, 1430433324, 1, 1430433324, 0, 1, 'Tìm thấy trung tá phi công trong buồng lái tiêm kích Su-22', 'Tim-thay-trung-ta-phi-cong-trong-buong-lai-tiem-kich-Su-22', 'Sau 12 ngày gặp nạn cùng đồng đội tại vùng biển Phú Quý &#40;Bình Thuận&#41;, trung tá Lê Văn Nghĩa được tìm thấy cách vị trí rơi chừng 100 mét. Bộ Quốc phòng tiếp tục tìm kiếm phi công Nguyễn Anh Tú. ', '2015_05/01/0309638823032-16.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(12, 1, '1', 0, 1, '', 1, 1430433422, 1430433559, 1, 1430433559, 0, 1, 'Lắp pháo hoa trên toà nhà cao nhất TP HCM', 'Lap-phao-hoa-tren-toa-nha-cao-nhat-TP-HCM', 'Hàng chục chuyên gia kỹ thuật cùng lực lượng Z121 đang tất bật lắp pháo hoa, chuẩn bị cho màn trình diễn chào mừng 40 năm thống nhất đất nước lúc 21h45 ngày 30&#x002F;4.', '2015_05/01/57825157962994-17.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(13, 1, '1', 0, 1, '', 1, 1430433422, 1430433167, 1, 1430433167, 0, 1, 'Hàng nghìn người Sài Gòn đến quảng trường đi bộ Nguyễn Huệ', 'Hang-nghin-nguoi-Sai-Gon-den-quang-truong-di-bo-Nguyen-Hue', 'Sáng 29&#x002F;4, phố đi bộ đầu tiên của Việt Nam mở cửa cho hàng nghìn người đến chụp ảnh, tham quan.', '2015_05/01/332531040342614-19.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(14, 1, '1', 0, 1, '', 1, 1430433422, 1430433495, 1, 1430433495, 0, 1, 'Năm người tử nạn khi xe Camry bị ôtô khách đâm bẹp dúm', 'Nam-nguoi-tu-nan-khi-xe-Camry-bi-oto-khach-dam-bep-dum', 'Sau cú tông ngang, xe khách đẩy chiếc Camry chở 7 người đi chừng 100 mét mới dừng lại. 5 nạn nhân tử vong, 2 người bị thương. Chiếc Camry bẹp dúm, gần như gãy đôi.', '2015_05/01/995912092909556-20.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(15, 1, '1', 0, 1, '', 1, 1430433422, 1430433442, 1, 1430433442, 0, 1, 'Người phụ nữ bị nước cuốn mất tích tại trung tâm Đà Lạt', 'Nguoi-phu-nu-bi-nuoc-cuon-mat-tich-tai-trung-tam-Da-Lat', 'Sau cơn mưa lớn, chị Duyên ra sau nhà dọn dẹp thì trượt chân xuống dòng suối đang chảy cuồn cuộn, mất tích.', '2015_05/01/01453455780247-21.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(16, 1, '1', 0, 1, '', 1, 1430433422, 1430433675, 1, 1430433675, 0, 1, 'Tìm thấy thi thể phi công thứ hai của tiêm kích Su-22', 'Tim-thay-thi-the-phi-cong-thu-hai-cua-tiem-kich-Su-22', 'Sau 13 ngày gặp nạn cùng đồng đội ở vùng biển Phú Qúy &#40;Bình Thuận&#41;, thi thể đại úy Nguyễn Anh Tú được tìm thấy cách vị trí tiêm kích Su-22 rơi hơn 200 mét.', '2015_05/01/895085435801487-23.jpg', '', 2, 1, '4', 1, 1, 0, 0, 0),
(17, 1, '1', 0, 1, '', 1, 1430433422, 1430433455, 1, 1430433455, 0, 1, '5 ôtô húc nhau tại cửa ngõ Sài Gòn', '5-oto-huc-nhau-tai-cua-ngo-Sai-Gon', 'Sáng 30&#x002F;4, xe khách được cho là lao tới đâm ôtô phía trước, gây tai nạn liên hoàn cho 3 xe nữa.', '2015_05/01/7598374999081-27.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(18, 1, '1', 0, 1, '', 1, 1430433422, 1430433447, 1, 1430433447, 0, 1, 'Những chính sách có hiệu lực từ tháng 5', 'Nhung-chinh-sach-co-hieu-luc-tu-thang-5', 'Hỗ trợ duy trì việc làm cho người lao động, kết luận thanh tra phải được kiểm tra, giám sát, văn phòng công chứng ở địa bàn khó khăn được ưu đãi... là những quy định có hiệu lực từ 1&#x002F;5.', '2015_05/01/73086314469855-28.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(19, 2, '2', 0, 1, '', 2, 1430433668, 1430433363, 1, 1430433363, 0, 1, 'Có một Việt Nam nằm trong tiềm thức', 'Co-mot-Viet-Nam-nam-trong-tiem-thuc', 'Mỗi sáng thức dậy tôi đều tự hỏi liệu may mắn của tôi rồi có mất đi&#33; Tôi có những đứa con rồi những đứa cháu bình thường và xinh đẹp, nhưng nhiều người bạn Việt Nam của tôi thì không được may mắn như vậy&#33;', '2015_05/01/74705146794806-30.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(20, 2, '2', 0, 1, '', 2, 1430433668, 1430433771, 1, 1430433771, 0, 1, 'Người Việt công tác ở Nepal hiến máu giúp nạn nhân động đất', 'Nguoi-Viet-cong-tac-o-Nepal-hien-mau-giup-nan-nhan-dong-dat', 'Sang công tác ở Nepal, hai thành viên của tập đoàn Viễn thông Quân đội &#40;Viettel&#41; đã tham gia hỗ trợ những nạn nhân của trận động đất ở đây. Phóng viên thực hiện phỏng vấn qua điện thoại với ông Nguyễn Thế Nghĩa, Phó Tổng giám đốc Công ty cổ phần Đầu tư Quốc tế &#40;Viettel Global&#41;.', '2015_05/01/82142336820055-31.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(21, 2, '2', 0, 1, '', 2, 1430433668, 1430433291, 1, 1430433291, 0, 1, 'Báo quốc tế đưa tin Việt Nam diễu binh mừng 40 năm thống nhất', 'Bao-quoc-te-dua-tin-Viet-Nam-dieu-binh-mung-40-nam-thong-nhat', 'Dân trí Sự kiện Việt Nam tổ chức long trọng lễ kỷ niệm mừng 40 năm giải phóng miền Nam, thống nhất đất nước đã được nhiều hãng tin, tờ báo hàng đầu thế giới đăng tải đậm nét, kèm theo những đánh giá cao về thành tựu kinh tế, xã hội những năm qua.', '2015_05/01/84689440917084-32.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(22, 2, '2', 0, 1, '', 2, 1430433668, 1430433479, 1, 1430433479, 0, 1, 'Phần Lan bắn bom cảnh báo vật thể lạ nghi là tàu ngầm Nga', 'Phan-Lan-ban-bom-canh-bao-vat-the-la-nghi-la-tau-ngam-Nga', 'Dân trí Hải quân Phần Lan ngày 29&#x002F;4 đã bắn bom gần một vật thể lạ mà nước này nghi là tàu ngầm của Nga ở khu vực lãnh hải gần với thủ đô của nước này.', '2015_05/01/72411487496783-33.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(23, 2, '2', 0, 1, '', 2, 1430433668, 1430433543, 1, 1430433543, 0, 1, 'Nghị sỹ Hữu nghị Mexico-Việt Nam kỷ nhiệm ngày 30&#x002F;4', 'Nghi-sy-Huu-nghi-Mexico-Viet-Nam-ky-nhiem-ngay-30-4', 'Dân trí Nhân dịp kỷ niệm 40 năm ngày giải phóng miền Nam, thống nhất đất nước Việt Nam &#40;30&#x002F;4&#x002F;1975-30&#x002F;4&#x002F;2015&#41;, Đại sứ quán Việt Nam cùng Hội Nghị sỹ Hữu nghị Mexico-Việt Nam tổ chức Triển lãm ảnh với chủ đề “Việt Nam ngày nay” tại trụ sở Hạ viện Liên bang Mexico.', '2015_05/01/748804860638007-34.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(24, 2, '2', 0, 1, '', 2, 1430433668, 1430433502, 1, 1430433502, 0, 1, 'Làn sóng biểu tình từ Baltimore lan rộng khắp nước Mỹ', 'Lan-song-bieu-tinh-tu-Baltimore-lan-rong-khap-nuoc-My', 'Biểu tình diễn ra rầm rộ ở các thành phố khác của New York, ở Washington, ở Nhà Trắng. Riêng ở Baltimore, 3.000 cảnh sát và vệ binh được huy động.', '2015_05/01/92634931926521-35.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(25, 2, '2', 0, 1, '', 2, 1430433668, 1430433509, 1, 1430433509, 0, 1, 'Nhật, Mỹ có thể tuần tra chung trên Biển Đông', 'Nhat-My-co-the-tuan-tra-chung-tren-Bien-Dong', 'Dân trí Nhật Bản có thể sẽ cùng Mỹ tham gia tuần tra chung trên không phận Biển Đông, nhằm ứng phó với ý định ngày một lộ rõ của Trung Quốc trong việc sử dụng những khu vực được bồi lấn trên vùng biển này, các nguồn tin từ giới chức hai nước cho biết.', '2015_05/01/88328757584699-36.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(26, 2, '2', 0, 1, '', 2, 1430433668, 1430433617, 1, 1430433617, 0, 1, 'Mỹ và Việt Nam nên là bạn', 'My-va-Viet-Nam-nen-la-ban', 'Cấm vận vũ khí sát thương với Việt Nam của Mỹ hiện tại là một biểu tượng có ý nghĩa về giá trị tinh thần hơn nhiều hơn vật chất.', '2015_05/01/2222969520328-37.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(27, 2, '2', 0, 1, '', 2, 1430433668, 1430433550, 1, 1430433550, 0, 1, 'Triều Tiên bị nghi đã tái khởi động lò phản ứng hạt nhân', 'Trieu-Tien-bi-nghi-da-tai-khoi-dong-lo-phan-ung-hat-nhan', 'Dân trí Những bức ảnh vệ tinh mới đây cho thấy lò phản ứng hạt nhân của Triều Tiên có vẻ đã hoạt động trở lại, một cơ quan nghiên cứu của Mỹ khẳng định.', '2015_05/01/125313137705933-38.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(28, 2, '2', 0, 1, '', 2, 1430433668, 1430433556, 1, 1430433556, 0, 1, 'Chiến thắng VN - bốn trong một&#x3A; Cái nhìn của người ngoài cuộc', 'Chien-thang-VN-bon-trong-mot-Cai-nhin-cua-nguoi-ngoai-cuoc', 'Nhân kỷ niệm 40 năm ngày thống nhất đất nước, xin giới thiệu với bạn đọc bản lược dịch bài viết của Chuyên gia quân sự Nga.', '2015_05/01/326542152772971-39.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(29, 2, '2', 0, 1, '', 2, 1430433668, 1430433571, 1, 1430433571, 0, 1, 'Nepal&#x3A; Thiếu niên sống sót thần kỳ sau 5 ngày bị vùi lấp', 'Nepal-Thieu-nien-song-sot-than-ky-sau-5-ngay-bi-vui-lap', 'Dân trí Một đội cứu hộ của Mỹ ngày 30&#x002F;4 đã giải cứu thành công một thiếu niên bị mắc kẹt 5 ngày dưới đống đổ nát, nhưng vẫn tỉnh táo nhờ tìm được thực phẩm. Trước đó, một bé trai mới 4 tháng tuổi cũng được cứu sống sau 22 giờ bị vùi lấp và tưởng đã chết.', '2015_05/01/571354682836-40.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(30, 3, '3', 0, 1, '', 2, 1430433896, 1430433721, 1, 1430433721, 0, 1, 'Luyện thi IELTS&#x002F;TOEFL iBT - Bí quyết để thành công', 'Luyen-thi-IELTS-TOEFL-iBT-Bi-quyet-de-thanh-cong', 'Việc đầu tư vào chứng chỉ IELTS hay TOEFL iBT hiện nay được xem là “đầu tư 3 trong 1” vì được miễn thi tốt nghiệp THPT môn ngoại ngữ, nâng cao cơ hội xin học bổng du học và tăng sức cạnh tranh khi xin việc. Vấn đề là phải lên kế hoạch như thế nào và chuẩn bị ra sao để đạt điểm IELTS hay TOEFL iBT cao?', '2015_05/01/8793414794177-41.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(31, 3, '3', 0, 1, '', 2, 1430433896, 1430433894, 1, 1430433894, 0, 1, 'Học bổng thạc sỹ toàn phần tại AIT Bangkok', 'Hoc-bong-thac-sy-toan-phan-tai-AIT-Bangkok', 'Dân trí Chương trình học bổng sẽ được triển khai trong 3 năm &#40;2015-2018&#41; với 14 suất học bổng mỗi năm. Mỗi học bổng bao gồm toàn bộ học phí và phí ghi danh &#40;725.000 bạt&#41; kèm chi phí ăn ở, sinh hoạt phí nội trú tại AIT &#40;300.000 bạt&#41; trong 22 tháng để hoàn tất chương trình thạc sỹ.', '2015_05/01/657735652534193-42.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(32, 3, '3', 0, 1, '', 2, 1430433896, 1430435217, 1, 1430435217, 0, 1, 'Nữ giảng viên “Đông trùng hạ thảo”', 'Nu-giang-vien-Dong-trung-ha-thao', 'Thạc sĩ Trịnh Thị Xuân, 33 tuổi, ở Khoa Nông nghiệp và Sinh học ứng dụng Trường Đại học Cần Thơ đã nghiên cứu thành công quy trình sản xuất Đông trùng hạ thảo.', '2015_05/01/40410179232035-43.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(33, 3, '3', 0, 1, '', 2, 1430433896, 1430435527, 1, 1430435527, 0, 1, 'Thi công chức&#x3A; Thủ khoa, thạc sĩ &quot;xịn&quot; trượt còn dài dài', 'Thi-cong-chuc-Thu-khoa-thac-si-xin-truot-con-dai-dai', 'Sau khi kiểm tra đề cương ôn tập và ngân hàng đề thi các năm trên mạng, tôi rất buồn khi nhận thấy tư duy thi cử vẫn không có gì thay đổi so với thời của tôi từ 20 năm trước.', '2015_05/01/7888719959986-44.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(34, 3, '3', 0, 1, '', 2, 1430433896, 1430435261, 1, 1430435261, 0, 1, 'Đất lành của trí thức', 'Dat-lanh-cua-tri-thuc', 'Rời bỏ điều kiện làm việc rất thuận lợi ở nước ngoài, nhiều trí thức nổi tiếng đã trở về cống hiến cho sự nghiệp giáo dục, đào tạo và nghiên cứu khoa học của TPHCM và cả nước.', '2015_05/01/9125460885988-45.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(35, 3, '3', 0, 1, '', 2, 1430433896, 1430435229, 1, 1430435229, 0, 1, '“Tiểu Mozart” thần đồng âm nhạc tốt nghiệp đại học ở tuổi 11', 'Tieu-Mozart-than-dong-am-nhac-tot-nghiep-dai-hoc-o-tuoi-11', 'Dân trí Curtis Elton bắt đầu chơi nhạc từ khi vừa tròn 3 tuổi và trở thành người trẻ nhất thế giới nhận tấm bằng Đại học về âm nhạc ở độ tuổi 11.', '2015_05/01/21231092374186-46.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(36, 3, '3', 0, 1, '', 2, 1430433896, 1430435283, 1, 1430435283, 0, 1, 'Thí sinh được chỉnh sửa sai sót trong hồ sơ đăng ký dự thi', 'Thi-sinh-duoc-chinh-sua-sai-sot-trong-ho-so-dang-ky-du-thi', 'Dân trí Ngày 30&#x002F;4 là hạn chót để thí sinh nộp hồ sơ đăng ký dự thi THPT quốc gia. Sau hạn nộp hồ sơ này, nếu thí sinh phát hiện có nhầm lẫn, sai sót, phải thông báo kịp thời cho Hiệu trưởng trường phổ thông hoặc Thủ trưởng đơn vị nơi đăng ký dự thi hoặc cho Hội đồng thi trong ngày làm thủ tục dự thi để sửa chữa, bổ sung.', '2015_05/01/058542529181451-47.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(37, 3, '3', 0, 1, '', 2, 1430433896, 1430435547, 1, 1430435547, 0, 1, 'Một thời nhà giáo đi B', 'Mot-thoi-nha-giao-di-B', 'Khởi đầu từ một ngôi làng ở Quảng Bình, bằng lối mòn len lỏi xuyên dải rừng già Trường Sơn, qua bao nhiêu dốc đèo sông suối hiểm trở, các nhà giáo đi B vượt Trường Sơn vào Nam công tác.', '2015_05/01/340549946053-48.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(38, 3, '3', 0, 1, '', 2, 1430433896, 1430435302, 1, 1430435302, 0, 1, 'Thấm thía giá trị hòa bình từ lễ tri ân', 'Tham-thia-gia-tri-hoa-binh-tu-le-tri-an', 'Lễ tri ân của học sinh đối với cha mẹ, thầy cô và đặc biệt với những người đã hy sinh xương máu để đất nước được thống nhất, hòa bình trong những ngày tháng 4 lịch sử thực sự ý nghĩa, giúp các em thêm yêu quý cuộc sống.', '2015_05/01/277509712689578-49.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(39, 3, '3', 0, 1, '', 2, 1430433896, 1430435408, 1, 1430435408, 0, 1, '“Cái nôi” đóng góp trí tuệ trực tiếp trên chiến trường', 'Cai-noi-dong-gop-tri-tue-truc-tiep-tren-chien-truong', 'Dân trí Chiến thắng 30&#x002F;4 là một mốc son vĩ đại của lịch sử dân tộc Việt Nam, chấm dứt hoàn toàn gần một thế kỷ ngoại xâm và bắt đầu giai đoạn xây dựng, phát triển đất nước trong hoà bình, độc lập. Hàng ngàn nhà khoa học, cán bộ, giảng viên, sinh viên đã đóng góp sức lực, tâm huyết, hy sinh xương máu cho thắng lợi cuối cùng của cuộc kháng chiến, để lại những dấu ấn đáng tự hào trong tiến trình vĩ đại ấy của lịch sử dân tộc.', '2015_05/01/67778325887786-50.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(40, 3, '3', 0, 1, '', 2, 1430433896, 1430435368, 1, 1430435368, 0, 1, 'Chuyện nhà khoa học nữ từng được đề cử Nobel Hòa bình', 'Chuyen-nha-khoa-hoc-nu-tung-duoc-de-cu-Nobel-Hoa-binh', 'Tên tuổi của GS. TS Lê Thị Quý được giới học thuật hết sức trân trọng. Bà là 1 trong hơn 10 người phụ nữ Việt Nam có tên trong danh sách 1.000 người phụ nữ được đề cử giải Nobel Hòa bình năm 2005 và được đề danh trong cuốn sách “1.000 phụ nữ đấu tranh vì hòa bình trên thế giới”.', '2015_05/01/872308686209-51.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(41, 4, '4', 0, 1, '', 2, 1430435803, 1430435787, 1, 1430435787, 0, 1, 'Bắt nghi phạm tạt xăng đốt nam sinh lớp 10', 'Bat-nghi-pham-tat-xang-dot-nam-sinh-lop-10', 'Dân trí Ngày 28&#x002F;4, Cơ quan CSĐT Công an quận 10, TPHCM cho biết đang tạm giữ hình sự Vương Tiến P. &#40;SN 2001, ngụ quận 6&#41; để điều tra về hành vi cố ý gây thương tích.', '2015_05/01/53831181187092-53.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(42, 4, '4', 0, 1, '', 2, 1430435803, 1430435570, 1, 1430435570, 0, 1, '7 trai làng vướng lao lý vì &quot;chuyền tay&quot; thiếu nữ 13 tuổi', '7-trai-lang-vuong-lao-ly-vi-chuyen-tay-thieu-nu-13-tuoi', 'Ở độ tuổi 13 nhưng Nguyễn Hà Linh đã phổng phao, xinh đẹp như một thiếu nữ mười tám đôi mươi. Vẻ xinh đẹp, trẻ trung cùng với tính… dễ dãi của Linh đã đưa 7 trai làng cùng vướng vòng lao lý, khi quan hệ tình dục với trẻ vị thành niên.', '2015_05/01/892340650656186-54.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(43, 4, '4', 0, 1, '', 2, 1430435803, 1430435470, 1, 1430435470, 0, 1, 'Lừa đảo bán gà giống Đông Tảo “dỏm”', 'Lua-dao-ban-ga-giong-Dong-Tao-dom', 'Dân trí Ngày 28&#x002F;4, tin từ Chi cục Thú y tỉnh Thừa Thiên Huế cho biết vừa lập biên bản vi phạm đối với một người bán gà con giả gà con Đông Tảo và không có giấy tờ kiểm dịch.', '2015_05/01/64315957313022-55.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(44, 4, '4', 0, 1, '', 2, 1430435803, 1430435633, 1, 1430435633, 0, 1, '2 “quý cô” sập bẫy hacker vì tin vào quà… ảo từ trời Tây', '2-quy-co-sap-bay-hacker-vi-tin-vao-qua-ao-tu-troi-Tay', 'Dân trí Nhóm tội phạm công nghệ cao &#40;hacker&#41; dựng lên “kịch bản” tán tỉnh làm quen các cô gái Việt trên mạng xin làm “bạn trai” rồi... tặng quà làm các cô gái nhẹ dạ tưởng thật chi tiền cho dịch vụ để rồi tiền mất, quà đâu chẳng thấy đâu đành ôm “quả đắng”.', '2015_05/01/564700883728474-56.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(45, 4, '4', 0, 1, '', 2, 1430435803, 1430435517, 1, 1430435517, 0, 1, 'Hà Nội&#x3A; Bắt đối tượng vụ tung đồn tin nữ sinh bị hiếp, giết để tăng view facebook', 'Ha-Noi-Bat-doi-tuong-vu-tung-don-tin-nu-sinh-bi-hiep-giet-de-tang-view-facebook', 'Dân trí Cơ quan điều tra xác định, Sơn và Bằng sử dụng các tài khoản facebook bị hack, tạo ra nhiều tên miền khác để đăng tin sai sự thật, khi người dùng truy cập vào sẽ chuyển hướng về các website cần tăng lượng truy cập với mục đích hưởng tiền quảng cáo của Google.', '2015_05/01/392288647418295-57.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(46, 4, '4', 0, 1, '', 2, 1430435803, 1430435877, 1, 1430435877, 0, 1, 'Bắt đối tượng trốn truy nã đang làm việc trên tàu đánh cá', 'Bat-doi-tuong-tron-truy-na-dang-lam-viec-tren-tau-danh-ca', 'Dân trí Sáng 29&#x002F;4, Trạm kiểm soát biên phòng Bình Thắng &#40;xã Bình Thắng, huyện Bình Đại, tỉnh Bến Tre&#41; đã phát hiện và tạm giữ Trương Hoàng Đông &#40;23 tuổi, ngụ xã Tân Thạch, huyện Châu Thành, Bến Tre&#41; là đối tượng trốn truy nã từ đầu tháng 4 năm 2015.', '2015_05/01/85831462956193-58.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(47, 4, '4', 0, 1, '', 2, 1430435803, 1430435706, 1, 1430435706, 0, 1, 'Triệt phá ổ xóc đĩa trong rừng tràm', 'Triet-pha-o-xoc-dia-trong-rung-tram', 'Dân trí Ngày 29&#x002F;4, Phòng CSĐT tội phạm về trật tự xã hội Công an tỉnh Quảng Nam cho biết vừa bắt giữ hơn 30 con bạc tham gia xóc đĩa ăn tiền trong rừng tràm tại xã Tam Thái, huyện Phú Ninh.', '2015_05/01/69395197162170-59.jpg', '', 2, 1, '4', 1, 1, 1, 0, 0),
(48, 4, '4', 0, 1, '', 2, 1430435803, 1430435697, 1, 1430435697, 0, 1, 'Triệt phá ổ nhóm chuyên cung cấp ma tuý cho vũ trường', 'Triet-pha-o-nhom-chuyen-cung-cap-ma-tuy-cho-vu-truong', 'Dân trí Các đối tượng thuê những ngôi nhà ở trong ngõ ngách, đường đi vào nhỏ hẹp, hiểm trở; ở giáp ranh giữa các quận, huyện, có nhiều ngã đường để dễ tẩu thoát. Bên ngoài, chúng lắp camera để theo dõi &quot;đề phòng&quot; khi lực lượng công an xuất hiện.', '2015_05/01/8038950217414-60.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(49, 4, '4', 0, 1, '', 2, 1430435803, 1430435460, 1, 1430435460, 0, 1, 'Kiếm tiền từ việc “câu view” bằng tin đồn thất thiệt', 'Kiem-tien-tu-viec-cau-view-bang-tin-don-that-thiet', 'Dân trí Mua lại các tài khoản facebook bị hack, Sơn và Bằng sử dụng các tài khoản này đăng tin giật gân, sai sự thật lên các nhóm để “câu view” cho các website cần tăng lượng người truy cập, từ đó kiếm được tiền từ dịch vụ quảng cáo của Google.', '2015_05/01/442213886723626-61.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(50, 4, '4', 0, 1, '', 2, 1430435803, 1430435894, 1, 1430435894, 0, 1, 'Gã cán bộ “dởm” lừa chạy việc chiếm đoạt hơn 1 tỷ đồng', 'Ga-can-bo-dom-lua-chay-viec-chiem-doat-hon-1-ty-dong', 'Dân trí Bằng thủ  đoạn “nổ” quen biết rất nhiều cán bộ cấp tỉnh, nên có khả năng chạy việc và chuyển công tác ở nhiều vị trí,  đối tượng Nguyễn Danh Thu đã dùng những lời &quot;có cánh&quot; để lừa đảo chiếm gần 1 tỷ đồng của các nạn nhân, rồi bỏ trốn.', '2015_05/01/4624371902760-62.jpg', '', 2, 1, '4', 1, 1, 1, 0, 0),
(51, 4, '4', 0, 1, '', 2, 1430435803, 1430435827, 1, 1430435827, 0, 1, 'Bà cụ 62 tuổi bị lừa mất 200 triệu đồng qua điện thoại', 'Ba-cu-62-tuoi-bi-lua-mat-200-trieu-dong-qua-dien-thoai', 'Mặc dù Bộ Công an, công an các tỉnh, thành đã cảnh báo về loại tội phạm sử dụng điện thoại giả mạo công an điều tra, lừa đảo chiếm đoạt tài sản, nhưng mới đây tại Bình Dương, một cụ bà 62 tuổi đã bị lừa chiếm đoạt 200 triệu đồng bằng thủ đoạn này.', '2015_05/01/841449170014322-63.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(60, 6, '6', 0, 1, '', 2, 1430436058, 1430435799, 1, 1430435799, 0, 1, 'Ngắm hình ảnh mang bầu hạnh phúc của cặp đôi đồng tính', 'Ngam-hinh-anh-mang-bau-hanh-phuc-cua-cap-doi-dong-tinh', 'Dân trí Cặp đôi sinh sống ở miền bắc Carolina - Melanie và Vanessa Roy đã tự hào chia sẻ những hình ảnh tuyệt vời trong thời gian mang thai. Họ muốn tạo cảm hứng cho những đôi vợ chồng đồng tính khác cũng quyết định sinh con.', '2015_05/01/262276456101-72.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(61, 6, '6', 0, 1, '', 2, 1430436058, 1430435663, 1, 1430435663, 0, 1, 'Vững vàng khi chồng ngoại tình', 'Vung-vang-khi-chong-ngoai-tinh', 'Dân trí &quot;Ban đầu khóc thầm, héo hắt, tham dò định cầu cứu 2 bên gia đình nhưng vô vọng thôi chị à, khi một người chồng sẵn sàng vứt bỏ vợ con hơn 10 năm sướng khổ để chạy theo cô gái trẻ ranh ma tìm mọi cách để moi tiền đại gia thì mọi lời khuyên nhủ chỉ như nước đổ đầu vịt... &quot;', '2015_05/01/4126055911654-73.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(62, 6, '6', 0, 1, '', 2, 1430436058, 1430435702, 1, 1430435702, 0, 1, 'Tình yêu có thực sự “đi qua dạ dày“?', 'Tinh-yeu-co-thuc-su-di-qua-da-day', 'Cũng có thể điều này đúng, nếu như tình yêu và dạ dày không “vênh” nhau vì một lý do nào đó…', '2015_05/01/79765557566344-74.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(63, 6, '6', 0, 1, '', 2, 1430436058, 1430435967, 1, 1430435967, 0, 1, 'Mẹ bảo...', 'Me-bao', 'Dân trí Mẹ bảo&#x3A; &quot;Con đừng dán mắt vào điện thoại và máy tính như thế. Mới sinh em bé xong phải cố nghỉ ngơi cho được tròn tháng con ạ&quot;, thì con gắt&#x3A; &quot;Kệ con chứ&#33; Thằng bé nó ngủ cả ngày như thế, con nằm mãi nhìn trần nhà sao chịu được&quot;.', '2015_05/01/869809907556-75.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(64, 6, '6', 0, 1, '', 2, 1430436058, 1430436096, 1, 1430436096, 0, 1, 'Những xao xuyến đa mang', 'Nhung-xao-xuyen-da-mang', 'Dân trí Có đôi khi trong một khoảnh khắc bình yên giữa đời thường, ta bỗng giật mình lướt qua một hình bóng thân quen đến lạ. Hình bóng ấy gợi nhắc ta nhớ đến một người đã không còn có thể gặp lại nữa, khiến lòng ta đang yên với hiện tại lại bồi hồi dậy sóng.', '2015_05/01/0916288239433-76.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(65, 6, '6', 0, 1, '', 2, 1430436058, 1430435725, 1, 1430435725, 0, 1, 'Thời buổi này làm sao giữ hôn nhân?', 'Thoi-buoi-nay-lam-sao-giu-hon-nhan', 'Dân trí Thời buổi ta vội yêu đương, vội kết hôn rồi vội vã chia đôi ngả, làm đám cưới thì dễ nhưng giữ lời thề sánh bước đến đầu bạc răng long lại thật khó khăn. Hãy cân nhắc những điều dưới đây nếu muốn duy trì hôn nhân bền vững.', '2015_05/01/079629060440-77.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(66, 6, '6', 0, 1, '', 2, 1430436058, 1430436013, 1, 1430436013, 0, 1, '“Sao anh không gặp em sớm hơn”', 'Sao-anh-khong-gap-em-som-hon', 'Dân trí Nàng xinh xắn, mỏng manh và nhẹ nhàng như cơn gió. Nàng sống kín đáo, ít giao du và có phần mơ mộng. Thế nên, nàng không dám cầu toàn, chỉ mong phu quân của mình là một đại trượng phu, đừng quá để tâm đến những điều nhỏ nhặt.', '2015_05/01/62969453233-78.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(67, 5, '5', 0, 1, '', 2, 1430436151, 1430436043, 1, 1430436043, 0, 1, 'Các nước thực hiện “quyền được chết” như thế nào?', 'Cac-nuoc-thuc-hien-quyen-duoc-chet-nhu-the-nao', 'Dân trí Dự thảo đưa Quyền được chết vào ộ luật Dân sự hiện đang gây ra nhiều tranh luận. Vậy quyền được chết hay “cái chết êm ái” được quốc tế nhìn nhận ra sao?', '2015_05/01/86344069460880-79.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(68, 5, '5', 0, 1, '', 2, 1430436151, 1430436166, 1, 1430436166, 0, 1, 'Một phụ nữ nhập viện do thời trẻ bơm ngực', 'Mot-phu-nu-nhap-vien-do-thoi-tre-bom-nguc', 'Một phụ nữ đang trong thời kỳ cho con bú đã phải nhập viện do ngực trái bị sưng to gấp 3 lần ngực phải. Nguyên nhân do thời còn trẻ, chị đi Hồng Kông bơm mỡ nhân tạo, đến khi cho con bú, ngực chị bị nhiễm trùng.', '2015_05/01/262481851743359-80.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(69, 5, '5', 0, 1, '', 2, 1430436151, 1430435778, 1, 1430435778, 0, 1, '8 nguyên tắc đơn giản tránh ngộ độc trong mùa hè', '8-nguyen-tac-don-gian-tranh-ngo-doc-trong-mua-he', 'Ngộ độc thực phẩm rất phổ biến vào mùa hè do thức ăn nhanh ôi thiu, dưới đây là những cách đơn giản tránh mắc phải bệnh này.', '2015_05/01/89216682294433-81.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(70, 5, '5', 0, 1, '', 2, 1430436151, 1430436177, 1, 1430436177, 0, 1, 'Quá chén rượu bia và những hệ lụy sức khỏe', 'Qua-chen-ruou-bia-va-nhung-he-luy-suc-khoe', 'Dân trí Trong những kỳ nghỉ lễ, gia đình, bạn bè, anh em đồng nghiệp gặp gỡ, thăm nom thường đi liền với chén rượu, cốc bia. Nếu không kiểm soát tốt, tình trạng quá chén thường xuyên xảy ra sẽ gây nhiều hệ lụy với cả người uống và xã hội.', '2015_05/01/3966191658103-82.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(71, 5, '5', 0, 1, '', 2, 1430436151, 1430436137, 1, 1430436137, 0, 1, 'Ban Dân y miền Nam&#x3A; Những hồi ức về một thời oanh liệt', 'Ban-Dan-y-mien-Nam-Nhung-hoi-uc-ve-mot-thoi-oanh-liet', 'Dân trí Chiến thắng lịch sử 30&#x002F;4&#x002F;1975 giải phòng miền Nam, thống nhất đất nước có sự đóng góp to lớn của những chiến sĩ “áo trắng”. Một thời oanh liệt nhưng cũng muôn vàn nguy hiểm, khó khăn đã được tái hiện qua hồi ức của chính các y bác sĩ trở về từ cuộc chiến.', '2015_05/01/284527232851576-83.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(72, 5, '5', 0, 1, '', 2, 1430436151, 1430436222, 1, 1430436222, 0, 1, 'Trị hen&#x3A; Chớ tin lang vườn&#33;', 'Tri-hen-Cho-tin-lang-vuon', 'Mặc dù bệnh hen gây ra những hậu quả nghiêm trọng nhưng sự hiểu biết của người bệnh về việc điều trị và kiểm soát cơn hen còn nhiều hạn chế.', '2015_05/01/99846331369924-84.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(73, 5, '5', 0, 1, '', 2, 1430436151, 1430436133, 1, 1430436133, 0, 1, 'Thực phẩm nhiễm vi khuẩn tụ cầu khiến hơn 700 công nhân nhập viện', 'Thuc-pham-nhiem-vi-khuan-tu-cau-khien-hon-700-cong-nhan-nhap-vien', 'Dân trí Cục  An toàn thực phẩm ngày 30&#x002F;4  cho biết, vụ ngộ độc thực phẩm khiến hơn 700 công nhân ở hai tỉnh Long An, Tiền Giang phải nhập viện, nghi ngờ do ăn phải thực phẩm nhiễm vi khuẩn tụ cầu.', '', '', 0, 1, '4', 1, 0, 0, 0, 0),
(74, 5, '5', 0, 1, '', 2, 1430436151, 1430435966, 1, 1430435966, 0, 1, 'Hoạt chất trong súp lơ xanh có thể điều trị thoái hóa khớp', 'Hoat-chat-trong-sup-lo-xanh-co-the-dieu-tri-thoai-hoa-khop', 'Dân trí Nghiên cứu mới đây gợi ý rằng loại thuốc mới chứa một hoạt chất được tìm thấy trong súp lơ xanh có thể mang lại “tiềm năng to lớn” trong việc điều trị căn bệnh thoái hóa khớp.', '2015_05/01/6841283958333-86.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(75, 5, '5', 0, 1, '', 2, 1430436151, 1430436021, 1, 1430436021, 0, 1, 'Vụ thực phẩm bẩn vào trường học&#x3A; Gần 50&#x25; bếp ăn không đủ điều kiện an toàn thực phẩm', 'Vu-thuc-pham-ban-vao-truong-hoc-Gan-50-bep-an-khong-du-dieu-kien-an-toan-thuc-pham', 'Dân trí Kết quả kiểm tra Công ty TNHH MTV Phú Nhật Hào &#40;địa chỉ tại 12&#x002F;3, tổ 1, KP Bình Khánh, phường Khánh Bình, thị xã Tân Uyên&#41; - đơn vị cung cấp thực phẩm cho các trường học trên địa bàn tỉnh Bình Dương cho thấy nhiều yếu tố không đảm bảo an toàn thực phẩm.', '', '', 0, 1, '4', 1, 0, 0, 0, 0),
(76, 5, '5', 0, 1, '', 2, 1430436151, 1430435943, 1, 1430435943, 0, 1, 'Cứu sống sản phụ bị nhau cài răng lược và băng huyết nặng', 'Cuu-song-san-phu-bi-nhau-cai-rang-luoc-va-bang-huyet-nang', 'Dân trí Ngày 30&#x002F;4, bác sĩ Nguyễn Quang Hùng, Trưởng khoa Sản, Bệnh viện Đa khoa Thiện Hạnh &#40;tỉnh Đắk Lắk&#41;, cho biết&#x3A; Bệnh viện vừa phẫu thuật thành công và cứu sống được sản phụ mang thai 27 tuần bị băng huyết, nhau cài răng lược xuyên bàng quang rất nguy hiểm.', '2015_05/01/307752938700458-88.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(77, 5, '5', 0, 1, '', 2, 1430436151, 1430436156, 1, 1430436156, 0, 1, 'Viêm phổi làm suy giảm thể chất và tinh thần người lớn tuổi', 'Viem-phoi-lam-suy-giam-the-chat-va-tinh-than-nguoi-lon-tuoi', 'Dân trí Theo một nghiên cứu mới, những người lớn tuổi mắc viêm phổi sẽ có nguy cơ cao đối mặt với những vấn đề lâu dài về thể chất và tinh thần, làm giảm khả năng tự chăm sóc bản thân.', '2015_05/01/41132940307910-89.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0),
(78, 5, '5', 0, 1, '', 2, 1430436151, 1430436162, 1, 1430436162, 0, 1, 'Khánh thành Trung tâm Ung bướu Chợ Rẫy “hạ hỏa” cho phía Nam', 'Khanh-thanh-Trung-tam-Ung-buou-Cho-Ray-ha-hoa-cho-phia-Nam', 'Dân trí Sau 2 năm khẩn trương xây dựng, Trung tâm Ung Bướu bệnh viện Chợ Rẫy với tổng vốn đầu tư 492 tỷ đồng, quy mô 250 giường bệnh đã chính thức đi vào hoạt động. Ngành Y tế kỳ vọng đây sẽ là bước ngoặt, giải quyết tình trạng quá tải bệnh nhân ung thư.', '2015_05/01/202488473005318-90.jpg', '', 2, 1, '4', 1, 0, 0, 0, 0);

